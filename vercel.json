{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/cat", "dest": "/api/cat.js" },
    { "src": "/dog", "dest": "/api/dog.js" },
    { "src": "/capybara", "dest": "/api/capybara.js" },
    { "src": "/add-image", "dest": "/api/add-image.js" }
  ]
}
